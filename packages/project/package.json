{
  "name": "@nori-dot-com/project",
  "version": "1.2.0",
  "description": "Nori supplier project specification and utilities",
  "keywords": [
    "nori"
  ],
  "author": "jaycenhorton <jaycen@nori.com>",
  "homepage": "https://nori.com",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nori-dot-eco/nori-dot-com.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "make:docs": "yarn run typedoc --readme none --excludeInternal --excludeExternals --plugin typedoc-plugin-markdown  src/specification.ts src/validation.ts src/utils/convertFromV2ToV1.ts",
    "make:schema": "`yarn bin`/typescript-json-schema src/specification.ts Project -o ./src/json/specification.json --strictNullChecks=true --required=true --titles=true --noExtraProps=true  --validationKeywords errorMessage validationRules",
    "build": "yarn run make:schema && tsc",
    "prepare": "yarn run build",
    "watch": "tsc -w -p ."
  },
  "bugs": {
    "url": "https://github.com/nori-dot-eco/nori-dot-com/issues"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.7",
    "typedoc": "^0.20.32",
    "typedoc-plugin-markdown": "^3.6.0"
  },
  "dependencies": {
    "@nori-dot-com/math": "*",
    "@nori-dot-com/ggit": "*",
    "@nori-dot-com/errors": "*",
    "@turf/helpers": "^6.3.0",
    "ajv": "^7.2.1",
    "ajv-errors": "^2.0.1",
    "typescript-json-schema": "^0.50.0",
    "wicket": "^1.3.6"
  }
}
